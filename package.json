{
    "name": "inquieta-dulzura",
    "version": "1.0.0",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "cd server && nodemon --exec ts-node src/index.ts",
        "dev:client": "cd client && vite",
        "build": "npm run build:server && npm run build:client",
        "build:server": "cd server && tsc",
        "build:client": "cd client && vite build",
        "test": "vitest",
        "test:server": "cd server && vitest",
        "test:client": "cd client && vitest",
        "test:coverage": "vitest --coverage"
    },
    "dependencies": {
        "axios": "^1.6.2",
        "bcrypt": "^5.1.1",
        "cloudinary": "^2.8.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.17.1",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.15.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@testing-library/user-event": "^14.5.1",
        "@types/bcrypt": "^5.0.2",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.13",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/multer": "^1.4.7",
        "@types/node": "^20.10.5",
        "@types/react": "^18.0.17",
        "@types/react-dom": "^18.0.6",
        "@types/react-router-dom": "^5.3.3",
        "@types/uuid": "^9.0.0",
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "jsdom": "^23.0.1",
        "nodemon": "^3.0.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.3",
        "vite": "^7.2.7",
        "vitest": "^4.0.15"
    }
}